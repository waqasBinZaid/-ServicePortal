<script src="~/Content/assets/plugins/bootstrap/bootstrap.js"></script>

<script>
    $(document).ready(function () {
    
        $("#btnsub").click(function () {
            CatId = $("#Cat").val();
            SubCat = $("#valueSubCat").val();
            debugger
            $.ajax({
                type: 'post',
                url: '/Inventory/SubCatSave',
                data: {
                    CatId: $("#Cat").val(),
                    SubCat: $("#valueSubCat").val(),
                },

                success: function (response) {
                    if (response == "alrdy") {
                        alert("Subcategory Already Exist");
                    }
                    else {
                        alert("Data Save Succesfully");

                        $.get("/Inventory/SubjectByClassJs", { id: $("#Cat").val() }, function (data) {

                            $("#CatID").empty();
                            $.each(data, function (index, row) {
                                $("#CatID").append(
                                    "<option value='" + row.id + "'>" + row.SubCat + "</option> "
                                )

                            });
                        });
                        
                    }

                }

            });
            return false
        });
        $("#btncat").click(function () {
            debugger
            $.ajax({
                type: 'post',
                url: '/Inventory/CatSave',
                data: $('#catform').serialize(),

                success: function (response) {
                    if (response == "alrdyct") {
                       
                        alert("Category Already Exist");
                    }
                    else {

                        alert("Data Save Succesfully");
                        
                        $.ajax({

                            url: '/Inventory/CatLoad',
                            
                            success: function (aa) {
                                debugger
                                console.log(aa);

                                $("#Cat").empty();
                                $.each(aa, function () {
                                    $("#Cat").append(
                                        "<option value='" + this.Value + "'>" + this.Text + "</option> "
                                    )
                                });


                            }

                        });
                    }
                }
   
            });
            return false;
        });
        $("#btnman").click(function () {

            $.ajax({
                type: 'post',
                url: '/Inventory/ManSave',
                data: $("#manform").serialize(),

                success: function (response) {
                    if (response == "alrdyct") {

                        alert("Category Already Exist");
                    }
                    else {
                        alert("Data Save Succesfully");
                        $.ajax({

                            url: '/Inventory/ManufacturerLoad',

                            success: function (aa) {
                                debugger
                                console.log(aa);

                                $("#manfac").empty();
                                $.each(aa, function () {
                                    $("#manfac").append(
                                        "<option value='" + this.Value + "'>" + this.Text + "</option> "
                                    )
                                });


                            }

                        });
                    }

                }

            });
            return false
        });
        $("#btnmd").click(function () {
            var Models = $("#modelname").val();
            var M_CatId = $("#Cat").val();
            var M_SubCatId = $("#CatID").val();
            var M_ManFacID = $("#manfac").val();
            if (M_CatId != "" & M_SubCatId != "" & M_ManFacID != "") {
                $.ajax({
                    type: 'post',
                    url: '/Inventory/ModelSave',
                    data: {
                        Models: $("#modelname").val(),
                        M_CatId: $("#Cat").val(),
                        M_SubCatId: $("#CatID").val(),
                        M_ManFacID: $("#manfac").val(),
                    },

                    success: function (response) {
                        if (response == "error") {

                            alert("Enter Model Name");
                        }
                        else if (response == "alrdyct") {
                            alert("This Model Is already Exist");
                        }
                        else {
                            alert("Data Save Succesfully");

                            $.get("/Inventory/ModalDropDown", {
                                CatId: $("#Cat").val(),
                                SubCatId: $("#CatID").val(),
                                ManfacId: $("#manfac").val()
                            },
                                function (data) {

                                    $("#ModelID").empty();
                                    $.each(data, function (index, row) {
                                        $("#ModelID").append(
                                            "<option value='" + row.id + "'>" + row.Models + "</option> "
                                        )

                                    });
                                });

                        }
                    }

                });
            }
            else {
                alert("Above Fields cannot be null")
            }
            return false
        });
        $("#manfac").change(function () {

            $.get("/Inventory/ModalDropDown", {
                CatId: $("#Cat").val(),
                SubCatId: $("#CatID").val(),
                ManfacId: $("#manfac").val()
            },
                function (data) {

                    $("#ModelID").empty();
                $.each(data, function (index, row) {
                    $("#ModelID").append(
                        "<option value='" + row.id + "'>" + row.Models + "</option> "
                    )

                });
            });
            return false
                    $.ajax({
                        type: 'post',
                        url: '/Inventory/ModleDropDown',
                        data: {

                        },

                        success: function (response) {

                        }

                    });
        });

        $("#Cat").change(function () {

            $.get("/Inventory/SubjectByClassJs", { id: $("#Cat").val() }, function (data) {

                $("#CatID").empty();
                $.each(data, function (index, row) {
                    $("#CatID").append(
                        "<option value='" + row.id + "'>" + row.SubCat + "</option> "
                    )

                });
            });
            return false

        });

        $("#btnaddattribute").click(function () {

            $("#modeltest").modal("show");
        });

        $("#catsubcat").click(function () {
            var radioValue = $("input[name='AttributeType']:checked").val();
            debugger

            $.ajax({

                type: 'post',
                url: '/Inventory/CatSubCatSave',
                data: {

                    CatID: $("#Cat").val(),
                    SubCatID: $("#CatID").val(),
                    AttTypeID: $("#AttTypeID").val(),
                    AttributeType: radioValue
                },

                success: function (response) {
                    if (response == "errorr") {
                        alert("Attribute Already Exist");
                    }
                    else{

                        console.log(response);
                        List();

                    }
                }

            });

            function List() {
                $.ajax({
                    url: '/Inventory/buteList',
                    success: function (response) {
                        $('#list').html(response);
                    }
                });
            }

        });

        $("#btnacces").click(function () {
            CatID = $("#Cat").val();
            SubCatID = $("#CatID").val();
            ModelID = $("#ModelID").val();
            AccessoryID = $("#AccessoryID").val();
            Description = $("#Description").val();
            debugger
            if (CatID == "" || SubCatID == "" || ModelID == "" || Description == "") {
                alert("Please Select Category , SubCategory , Model , and Description")
            }
            else {

            $.ajax({

                type: 'post',
                url: '/Inventory/AccesorySave',
                data: {

                    CatID: $("#Cat").val(),
                    SubCatID: $("#CatID").val(),
                    ModelID: $("#ModelID").val(),
                    AccessoryID: $("#AccessoryID").val(),
                    Description: $("#Description").val()

                },

                success: function (res) {
                    if (res == "errorr") {
                        alert("Accessory Already Exist");
                    }
                    else {

                        console.log(res);
                        AccesList();

                    }
                }

            });
            }
            function AccesList() {
                $.ajax({
                    url: '/Inventory/AccessoryListReturn',
                    success: function (res) {
                        $('#acclist').html(res);
                    }
                });
            }
            return false
        });
        $("#btnaccesories").click(function () {

            $.ajax({

                type: 'post',
                url: '/Inventory/AccessoryList',
                data: {

                    CatID: $("#Cat").val(),
                    SubCatID: $("#CatID").val(),
                    ModelID: $("#ModelID").val(),
                   
                },
                success: function (res) {
                    $('#acclist').html(res);
                }
            });
        
        });

        $("#btnaddattribute").click(function () {
            $.ajax({

                type: 'post',
                url: '/Inventory/catsubcat',
                data: {

                    CatID: $("#Cat").val(),
                    SubCatID: $("#CatID").val(),

                },

                success: function (response) {
                    $('#list').html(response);
                }

            });

        });

        $("#btnatval").click(function () {

            $.ajax({
                type: 'post',
                url: '/Inventory/SaveItemAttribute',
                data: $('#itmvalform').serialize(),

                success: function (response) {
                    if (response == "itmatt") {

                        alert("Attribute Already Exist")
                    }
                    else {

                    alert("Data Save Succesfully");
                    $("#attrival").modal("hide");

                    }
                }

            });
            return false

        });

        $("#btnattributevalue").click(function () {
            var Cat = $("#Cat").val();
            var subcat = $("#CatID").val();
            var ModelID = $("#ModelID").val();
           
            if (ModelID == null) {
                alert("Please Select Model First")
            }
            else if (Cat != "" & subcat != "") {

                $("#modalvalag").modal("show");

                $.ajax({
                    type: 'post',
                    url: '/Inventory/CatSubCatAgainstModal',
                    data: {

                        Cat: $("#Cat").val(),
                        SubCat: $("#CatID").val(),
                        ModelID: $("#ModelID").val()
                    },

                    success: function (response) {

                        $('#noo').html(response);
                    }

                });
            }
            else {
                alert("Please select Category Or sub category to complete this process");
            }

            return false
        });

        $("#btnattributevalue").click(function () {
            var Cat = $("#Cat").val();
            var subcat = $("#CatID").val();
            var ModelID = $("#ModelID").val();
           
                $.ajax({
