@model ServicePortal.Models.inventryItem
@{
    ViewBag.Title = "NewInv";
    Layout = "~/Views/Shared/HeadofficeMasterPage.cshtml";
}

<div class="panel panel-default">
    <div class="panel-heading">
        Add New Items
        @{

            string x = Session["UpdateSubcatName"].ToString();
            string ModelVal = Session["updateModel"].ToString();
            string Category = Session["UpdatCatName"].ToString();
        }

    </div>
    <div class="panel-body">
        <div class="table-responsive">

            <div class="form-inline">
                @using (Html.BeginForm("Save", "Inventory", FormMethod.Post, new { enctype = "multipart/form-data" }))
                {
                    @Html.HiddenFor(m => m.id);
                    if (TempData["itemalready"] != null)
                    {
                        <div class="alert alert-warning alert-dismissable">
                            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>

                            <div class="alert alert-danger">
                                @TempData["itemalready"]
                            </div>

                        </div>
                    }

                    <div class="col-md-12">
                        <div class="col-md-4">
                            <div id="divss">
                                <div class="form-group has-success" id="lddiv">

                                    <label class="control-label" for="inputSuccess">Category</label>
                                    <br>

                                    @Html.DropDownList("Cat", ViewBag.Cat as List<SelectListItem>, new { @class = "form-control", placeholder = "Category", required = "required" })
                                    @Html.TextBox("nbb", Category, new { @class = "form-control", id = "cattxt" })
                                    <a href="#" class="form-control" id="btncat2" data-target="#myModal2" data-toggle="modal">+</a>

                                </div>

                                <div class="form-group has-success" id="lddiv2">

                                    <label class="control-label" for="inputSuccess">Sub Category</label>
                                    <br>
                                    @Html.DropDownListFor(m => m.CatID, new SelectList(""), "Select SubCatgory", new { @class = "form-control", id = "CatID" , required = "required" })
                                    <a href="#" class="form-control" id="newcat" data-target="#myModal" data-toggle="modal">+</a>
                                    <input type="button" class="btn btn-default" value="view Attribute" id="btnaddattribute">
                                </div>

                                <div class="form-group has-success" id="lddiv3">
                                    <label class="control-label" for="inputSuccess">Sub Category</label>
                                    <br>
                                    @Html.TextBox("abc", x, new { @class = "form-control", id = "catidtxt" })

                                </div>

                                <div class="form-group has-success" id="manfact">

                                    <label class="control-label" for="inputSuccess">Manfacturer</label>
                                    <br>
                                    @Html.DropDownListFor(m => m.ManFaceID, ViewBag.Manfacturer as List<SelectListItem>, new { @class = "form-control", placeholder = "Category", id = "manfac", required = "required" })
                                    <a href="#" class="form-control" id="newman" data-target="#manmodal" data-toggle="modal">+</a>

                                </div>

                                
                                <div class="form-group has-success">

                                    <label class="control-label" for="inputSuccess">Models</label>
                                    <br>
                                    @Html.DropDownListFor(m => m.ModelID, new SelectList(""), "Select Model", new { @class = "form-control", id = "ModelID"})
                                    @Html.TextBox("xyz", ModelVal, new { @class = "form-control", id = "mdtxt" })
                                    <a href="#" id="newmd" class="form-control" data-target="#myModalmd" data-toggle="modal">+</a>
                                    <input type="button" class="btn btn-default" value="Attribute Value" id="btnattributevalue"><br><br>
                                    <a href="#" id="btnaccesories" class="btn btn-outline btn-danger btn-block" data-target="#accval" data-toggle="modal">Click To Add Accessories</a>


                                </div>
                            </div>
                            <div class="form-group has-success">

                                <label class="control-label" for="inputSuccess">Serial No</label>
                                <br>
                                @Html.RadioButtonFor(m => m.SerialNo, "Yes") Required
                                @Html.RadioButtonFor(m => m.SerialNo, "No", new { @checked = "true" }) Not Required


                            </div>

                        </div>

                        <div class="col-md-4">
                            <div class="form-group has-success">

                                <label class="control-label" for="inputSuccess">Avg.Sale Price</label>
                                <br>
                                @Html.TextBoxFor(m => m.AvgSalePrice, null, new { @class = "form-control", id = "inputSuccess", placeholder = "Avg.Sale Price", type = "Text" })
                            </div>
                            <div class="form-group has-success">

                                <label class="control-label" for="inputSuccess">Avg.Purchase Price</label>
                                <br>
                                @Html.TextBoxFor(m => m.AvgPurchasePrice, null, new { @class = "form-control", id = "inputSuccess", placeholder = "Avg.PurchasePrice" })

                            </div>

                            <div class="form-group has-success">

                                <label class="control-label" for="inputSuccess">Bottom Price</label>
                                <br>
                                @Html.TextBoxFor(m => m.BottemPrice, null, new { @class = "form-control", id = "inputSuccess", placeholder = "BottomPrice"})

                            </div>
                            <div class="form-group has-success">

                                <label class="control-label" for="inputSuccess">Internal Price</label>
                                <br>
                                @Html.TextBoxFor(m => m.InternalPrice, null, new { @class = "form-control", id = "inputSuccess", placeholder = "Internal Price", type = "Text"})

                            </div>

                            <div class="form-group has-success">

                                <label class="control-label" for="inputSuccess">Picture</label>
                                <br>
                                @Html.TextBoxFor(m => m.Image, null, new { @class = "form-control", id = "inputSuccess", placeholder = "Phone", type = "file" })
                            </div>
                        </div>
                        <div class="col-md-4">

                            <div class="form-group has-success">

                                <label class="control-label" for="inputSuccess">Trade Price</label>
                                <br>
                                @Html.TextBoxFor(m => m.TradesPrice, null, new { @class = "form-control", id = "inputSuccess", placeholder = "Trader Price", type = "Text", })
                            </div>

                            <div class="form-group has-success">
 
                                <label class="control-label" for="inputSuccess">Quoted Price</label>
                                <br>
                                @Html.TextBoxFor(m => m.QoutedPrice, null, new { @class = "form-control", id = "qtpr", placeholder = "Quoted Price", type = "Text" })
                            </div>

                            <div class="form-group has-success">

                                <label class="control-label" for="inputSuccess">Sales Description</label>
                                <br>
                                @Html.TextAreaFor(m => m.SalesDescription, new { @class = "form-control", id = "inputSuccess", placeholde = "Description", @row = "10" })
                            </div>

                            <div class="form-group has-success">

                                <label class="control-label" for="inputSuccess">Purchase Description</label>
                                <br>
                                @Html.TextAreaFor(m => m.PurchaseDescription, new { @class = "form-control", id = "inputSuccess", placeholde = "Description", @row = "10" })
                            </div>
                        </div>

                    </div>

                    <div class="form-group has-success">
                        <br>
                        <button type="submit" class="btn btn-primary">Click To Save</button>

                    </div>
                }

            </div>

        </div>

    </div>
</div>
